{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm ci --include=dev",
  "env": {
    "NODE_ENV": "production",
    "SKIP_DB_INIT": "true",
    "VERCEL": "true"
  },
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "dist/**/*"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
